<apex:page controller="LeadCustomController">
    <header>
        <nav>
            <ul>
                <li><apex:outputLink value="LeadViewPage">View Accounts</apex:outputLink></li>
                <li><apex:outputLink value="LeadCreateEditPage">Create Account</apex:outputLink></li>
            </ul>
        </nav>
    </header>
    <apex:form >
        <apex:pageMessages/>
        <apex:pageBlock >
            <apex:pageBlockSection>
                <apex:inputField value="{!lead.FirstName}"/>
                <apex:inputField value="{!lead.LastName}"/>
                <apex:inputField value="{!lead.Company}"/>
                <apex:inputField value="{!lead.Status}"/>
                <apex:commandLink action="{!save}" value="Save">
                </apex:commandLink>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>